{
  "name": "sensemaker",
  "version": "0.1.0-dev",
  "description": "crawl and aggregate data.",
  "main": "services/sensemaker.js",
  "scripts": {
    "coverage": "c8 npm test",
    "serve:docs": "ecstatic docs",
    "docs": "npm run make:docs && npm run serve:docs",
    "make:api": "jsdoc2md --configure jsdoc.json --files types/*.js services/*.js README.md > API.md",
    "make:docs": "npm run make:api && node_modules/.bin/jsdoc -c jsdoc.json services/**.js types/**.js node_modules/@fabric/core/types/service.js DEVELOPERS.md -d docs/ && glob-run js-beautify --indent-size 2 -r docs/**.html",
    "start": "node scripts/node.js",
    "test": "mocha tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FabricLabs/sensemaker.git"
  },
  "keywords": [
    "aggregation",
    "ai",
    "artificial intelligence",
    "bitcoin",
    "cryptocurrency",
    "ethereum",
    "fabric",
    "intelligence",
    "lightning",
    "machine learning",
    "media",
    "network",
    "networking",
    "neural networks",
    "payments",
    "social media",
    "social network",
    "social"
  ],
  "author": "Fabric Labs",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/FabricLabs/sensemaker/issues"
  },
  "homepage": "https://github.com/FabricLabs/sensemaker",
  "dependencies": {
    "@fabric/core": "FabricLabs/fabric#master",
    "@fabric/ethereum": "FabricLabs/fabric-ethereum#feature/node-16",
    "@fabric/http": "FabricLabs/fabric-http#feature/node-16",
    "@fabric/matrix": "FabricLabs/fabric-matrix#develop",
    "@portal/feed": "FabricLabs/portal-feed#feature/federation",
    "fast-json-patch": "3.1.0",
    "levelgraph": "3.0.0",
    "lodash.merge": "4.6.2"
  }
}
