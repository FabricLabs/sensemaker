    "mysql2": "=3.12.0",
    "node-ipc": "=9.1.1",
    "node-util": "=0.0.1",
    "nodemailer": "=6.10.0",
    "openai": "=4.80.1",
    "path-browserify": "=1.0.1",
    "pdf-parse": "=1.1.1",
